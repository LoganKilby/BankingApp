package domain;

public class Account {
	private int id;
	private int balance;
	private String accountType;
	private String accountStatus;
	private int owner;
	private int jointOwner;
	
	public Account(int id, int balance, String accountType, String accountStatus, int owner, int jointOwner) {
		super();
		this.id = id;
		this.balance = balance;
		this.accountType = accountType;
		this.accountStatus = accountStatus;
		this.owner = owner;
		this.jointOwner = jointOwner;

	}

	@Override
	public String toString() {
		return "id: " + id + ", Balance: " + balance + ", Account Type: " + accountType + ", Account Status: " + accountStatus + ", Owner: "
				+ owner + ", Joint Owner: " + jointOwner + "]";
	}
	
	
}
